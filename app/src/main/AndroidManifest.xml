<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aazykj.wojh">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".app.MyApplication"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.Activity_Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name=".activity.Activity_Signin"></activity>
        <activity android:name=".activity.Activity_Main"
            android:windowSoftInputMode="stateHidden|stateAlwaysHidden"></activity>

    </application>

</manifest>
